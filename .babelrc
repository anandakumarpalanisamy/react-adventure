{
  "presets":
  [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "IE >= 9"
          ]
        },
        "useBuiltIns": true
      }
    ],
    "stage-1",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    "transform-decorators-legacy",
    "dynamic-import-node",
    ["webpack-alias", { "config": "./webpack.config.js" }]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "transform-react-constant-elements"
      ]
    },
    "test": {
      "plugins": [
        "transform-runtime",
        "transform-class-properties",
        ["webpack-alias", { "config": "./webpack.config.js" }]
      ]
    }
  }
}
